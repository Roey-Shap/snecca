// https://help.yoyogames.com/hc/en-us/articles/360005277377 for more information
function set_object_index_color(index){
	switch(index){
		case 0:
			return(c_white);
			break;
			
		case 1:
			return(SNAKE_COLOR);
			break;
			
		case 2:
			return(CHAR_COLOR);
			break;
		
	}
}